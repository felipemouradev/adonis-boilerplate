'use strict'

const Lucid = use('Lucid')

class [MODEL] extends Lucid {
  static get table () {
    return '[TABLE]'
  }
  static get primaryKey () {
    return '[PRIMARY]'
  }
  static get createTimestamp () {
    return null
  }
  static get updateTimestamp () {
    return null
  }

  static get rules () {
      return {
[RULES]
      }
  }

[USER_RESERVED]

}

module.exports = [MODEL]
